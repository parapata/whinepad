{
  "name": "whinepad",
  "version": "1.0.0",
  "main": "app.js",
  "license": "MIT",
  "babel": {
    "presets": [
      "es2015",
      "react",
      "stage-0"
    ]
  },
  "scripts": {
    "start": "webpack-dev-server --content-base public/",
    "build": "webpack -d --progress",
    "test:run": "jest",
    "test:coverage": "jest --coverage",
    "flow-type:install": "flow-typed install"
  },
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-jest": "^20.0.0",
    "babel-loader": "^7.0.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "classnames": "^2.2.5",
    "eslint": "^3.19.0",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-react": "^7.0.0",
    "fbemitter": "^2.1.1",
    "flow-babel-webpack-plugin": "^1.0.1",
    "flow-typed": "^2.1.2",
    "flux": "^3.1.2",
    "immutable": "^3.8.1",
    "invariant": "^2.2.2",
    "jest-cli": "^20.0.0",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "webpack": "^2.5.0",
    "webpack-dev-server": "^2.4.5"
  },
  "jest": {
    "transform": {
      ".*": "<rootDir>/node_modules/babel-jest"
    },
    "unmockedModulePathPatterns": [
      "node_modules/react",
      "node_modules/react-dom",
      "node_modules/fbjs"
    ]
  },
  "dependencies": {}
}
